<script>
  const test = JSON.parse(document.getElementById("dayData")?.dataset.day ?? "");
  var svgNS = "http://www.w3.org/2000/svg";
  test.day1.forEach((spot: { id: string; occupied: any; }) => {
    const spotId = "spot-" + spot.id;
    var elem = document.getElementById(spotId);
    if (elem !== null) {
      var newText = document.createElementNS(svgNS, "text");
      newText.setAttributeNS(
        null,
        "x",
        (parseFloat(elem.getAttribute("x") ?? "0") + 10).toString(),
      );
      newText.setAttributeNS(
        null,
        "y",
        (parseFloat(elem.getAttribute("y") ?? "0") + 42).toString(),
      );
      newText.setAttributeNS(null, "font-size", "40");
      var textNode = document.createTextNode(spot.id);
      newText.appendChild(textNode);
      document.getElementById("maingroup-day2")?.appendChild(newText);
      elem.style.fill = spot.occupied ? "#cf6251" : "#3c803c";
    }
  });
</script>

